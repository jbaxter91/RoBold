{
    "manifest_version": 2,
    "name": "RoBold - Bionic Reading",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Boldify First Letter",
      "default_popup": "popup.html"
    }
  }
  